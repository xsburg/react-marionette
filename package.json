{
  "name": "react-marionette",
  "version": "0.0.1",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "typings": "src/index.d.ts",
  "repository": "https://github.com/xsburg/react-marionette",
  "author": "Stepan Burguchev <me@sburg.net>",
  "license": "MIT",
  "scripts": {
    "compile": "cross-env BABEL_ENV=rollup rollup -c",
    "test": "npm run test:code && npm run test:typescript",
    "test:coverage": "jest --coverage",
    "test:code": "jest",
    "test:typescript": "typings-tester --dir test/typescript",
    "prepublish": "node ./prepublish"
  },
  "files": [
    "dist",
    "src/index.d.ts"
  ],
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^22.1.4",
    "jquery": "^3.3.1",
    "prettier": "^1.10.2",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.2",
    "typescript": "^2.4.2",
    "typings-tester": "^0.2.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "@types/react": "16.0.31",
    "backbone.marionette": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.jsx?)$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  }
}
